{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "defaultDestination": "wwwroot/",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "files": [
        "jquery.min.js",
        "jquery.js",
        "jquery.min.map"
      ],
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "unpkg",
      "library": "font-awesome-5-css@5.4.1",
      "destination": "wwwroot/lib/fontawesome"
    },
    {
      "library": "popper.js@1.14.7",
      "destination": "wwwroot/lib/popper"
    },
    {
      "library": "jquery.nicescroll@3.7.6",
      "destination": "wwwroot/lib/nicescroll"
    },
    {
      "library": "tooltip.js@1.3.1",
      "destination": "wwwroot/lib/tooltip"
    },

    {
      "library": "moment.js@2.24.0",
      "destination": "wwwroot/lib/moment"
    }
  ]
}